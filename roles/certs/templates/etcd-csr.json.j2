{
    "CN":"etcd",
    "hosts":[
        {% for host in groups['etcd'] %}
        "{{host}}",
        {% endfor %}
        "127.0.0.1"
    ],
    "key":{
        "algo":"rsa",
        "size":2048
    },
    "names":[
        {
            "C":"CN",
            "ST":"{{cert.names.st }}",
            "L":"{{cert.names.l}}",
            "O":"{{cert.names.o}}",
            "OU":"{{cert.names.ou}}"
        }
    ]
}